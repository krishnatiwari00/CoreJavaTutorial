package opps;

public class Addition1 {
	public void addValues(int a, int b)
	{
		int c = a+b;
	}
	
	public void addValues(int a, int b, int c)
	{
		int d = a+b+c;
	}
	
	public void addValues(double a, double b)
	{
		double c = a+b;
	}
	
	public String addName(String a, String b)
	{
		String c = a+b;
		return c;
	}

}
