package string;

public class StringMethodPart3_4 {

	public static void main(String[] args) {
		
		String text = "Hello Team";
		String Sample = "His name is john he is very good man";
		
		String text1 = text.replace("e", "a");
		System.out.println(text1);
		
		String SampleTest = Sample.replace("is", "was");
		System.out.println(SampleTest);
	}

}
